<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.1" xmlns="http://xmlns.jcp.org/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/persistence http://xmlns.jcp.org/xml/ns/persistence/persistence_2_1.xsd">
  <persistence-unit name="cnPU" transaction-type="RESOURCE_LOCAL">
    <provider>org.eclipse.persistence.jpa.PersistenceProvider</provider>
    <class>com.control.entity.Alumnos</class>
    <class>com.control.entity.Colegio</class>
    <class>com.control.entity.Empleados</class>
    <class>com.control.entity.Encargados</class>
    <class>com.control.entity.Especialidad</class>
    <class>com.control.entity.Evaluacion</class>
    <class>com.control.entity.Materias</class>
    <class>com.control.entity.Matricula</class>
    <class>com.control.entity.Opcion</class>
    <class>com.control.entity.Roles</class>
    <class>com.control.entity.TipoMatricula</class>
    <class>com.control.entity.Usuarios</class>
    <class>com.control.entity.OpcionEspe</class>
    <class>com.control.entity.Opciones</class>
    <class>com.control.entity.Estados</class>
    <exclude-unlisted-classes>false</exclude-unlisted-classes>
    <properties>
      <property name="javax.persistence.jdbc.url" value="jdbc:mysql://localhost:3306/BDCNE?zeroDateTimeBehavior=convertToNull"/>
      <property name="javax.persistence.jdbc.user" value="root"/>
      <property name="javax.persistence.jdbc.driver" value="com.mysql.jdbc.Driver"/>
      <property name="javax.persistence.jdbc.password" value="root"/>
      <property name="javax.persistence.schema-generation.database.action" value="create"/>
    </properties>
  </persistence-unit>
</persistence>
